{
  "title": "OTel Playground - Overview",
  "uid": "otel-playground-overview",
  "tags": ["opentelemetry", "playground", "observability"],
  "timezone": "browser",
  "schemaVersion": 38,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "quantile",
        "type": "custom",
        "label": "Percentile",
        "options": [
          { "text": "p50", "value": "0.50", "selected": false },
          { "text": "p95", "value": "0.95", "selected": true },
          { "text": "p99", "value": "0.99", "selected": false }
        ],
        "current": { "text": "p95", "value": "0.95" },
        "multi": false
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "enable": true,
        "expr": "changes(http_server_requests_total[1m]) > 0",
        "name": "Deployment Events",
        "tagKeys": "deployment",
        "textFormat": "Deployment detected",
        "titleFormat": "Deployment"
      }
    ]
  },
  "panels": [
    {
      "id": 100,
      "type": "row",
      "title": "📊 Service Health Overview",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "id": 1,
      "type": "stat",
      "title": "Requests/sec",
      "description": "Current request rate (QPS)",
      "gridPos": { "h": 5, "w": 4, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_total[1m]))",
          "refId": "A",
          "legendFormat": "QPS"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 10, "color": "yellow" },
              { "value": 50, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "orientation": "auto",
        "textMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Error Rate",
      "description": "Percentage of 5xx responses",
      "gridPos": { "h": 5, "w": 4, "x": 4, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_total{http_response_status_code=~\"5..\"}[5m])) / sum(rate(http_server_requests_total[5m]))",
          "refId": "A",
          "legendFormat": "Error Rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "decimals": 2,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 0.01, "color": "yellow" },
              { "value": 0.05, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "orientation": "auto",
        "textMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Latency (p95)",
      "description": "95th percentile response time",
      "gridPos": { "h": 5, "w": 4, "x": 8, "y": 1 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_request_duration_seconds_bucket[5m])))",
          "refId": "A",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 0.5, "color": "yellow" },
              { "value": 1, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "orientation": "auto",
        "textMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Active Requests",
      "description": "Currently in-flight requests",
      "gridPos": { "h": 5, "w": 4, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "sum(http_server_active_requests)",
          "refId": "A",
          "legendFormat": "Active"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "blue" },
              { "value": 5, "color": "yellow" },
              { "value": 10, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "orientation": "auto",
        "textMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Success Rate",
      "description": "Percentage of 2xx/3xx responses",
      "gridPos": { "h": 5, "w": 4, "x": 16, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_total{http_response_status_code=~\"[23]..\"}[5m])) / sum(rate(http_server_requests_total[5m]))",
          "refId": "A",
          "legendFormat": "Success Rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "decimals": 2,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 0.95, "color": "yellow" },
              { "value": 0.99, "color": "green" }
            ]
          },
          "max": 1,
          "min": 0
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "orientation": "auto",
        "textMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Total Requests (5m)",
      "description": "Total requests in last 5 minutes",
      "gridPos": { "h": 5, "w": 4, "x": 20, "y": 1 },
      "targets": [
        {
          "expr": "sum(increase(http_server_requests_total[5m]))",
          "refId": "A",
          "legendFormat": "Total"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "orientation": "auto",
        "textMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 200,
      "type": "row",
      "title": "🚀 Request Latency",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 6 },
      "collapsed": false
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "HTTP Server Latency (All Percentiles)",
      "description": "Response time distribution with exemplar trace links",
      "gridPos": { "h": 9, "w": 16, "x": 0, "y": 7 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(http_server_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p50 (median)",
          "refId": "A",
          "exemplar": true
        },
        {
          "expr": "histogram_quantile(0.90, sum by (le) (rate(http_server_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p90",
          "refId": "B",
          "exemplar": true
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p95",
          "refId": "C",
          "exemplar": true
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(http_server_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p99",
          "refId": "D",
          "exemplar": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 8,
            "gradientMode": "opacity",
            "spanNulls": true,
            "showPoints": "never",
            "pointSize": 5
          },
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "p99" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "p95" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "p50 (median)" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "calcs": ["mean", "max", "last"],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      }
    },
    {
      "id": 8,
      "type": "gauge",
      "title": "Current p95 Latency",
      "description": "Real-time 95th percentile latency with SLO threshold",
      "gridPos": { "h": 9, "w": 8, "x": 16, "y": 7 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_request_duration_seconds_bucket[1m])))",
          "refId": "A",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 0.3, "color": "yellow" },
              { "value": 0.5, "color": "orange" },
              { "value": 1, "color": "red" }
            ]
          },
          "max": 2,
          "min": 0
        }
      },
      "options": {
        "orientation": "auto",
        "showThresholdLabels": true,
        "showThresholdMarkers": true,
        "text": {
          "titleSize": 16,
          "valueSize": 48
        }
      }
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "Latency by Route (p95)",
      "description": "Compare performance across different endpoints with exemplars",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, http_route) (rate(http_server_request_duration_seconds_bucket[5m])))",
          "legendFormat": "{{http_route}}",
          "refId": "A",
          "exemplar": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "hue",
            "spanNulls": true,
            "showPoints": "auto",
            "pointSize": 5
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "legend": {
          "calcs": ["mean", "max", "last"],
          "displayMode": "table",
          "placement": "right",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      }
    },
    {
      "id": 300,
      "type": "row",
      "title": "⚠️ Errors & Traffic",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 24 },
      "collapsed": false
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "Error Rate Over Time",
      "description": "5xx error rate with alerting thresholds",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 25 },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_total{http_response_status_code=~\"5..\"}[5m])) / sum(rate(http_server_requests_total[5m]))",
          "legendFormat": "Error Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "spanNulls": true,
            "showPoints": "auto"
          },
          "color": {
            "mode": "fixed",
            "fixedColor": "red"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 0.01, "color": "yellow" },
              { "value": 0.05, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "legend": {
          "calcs": ["mean", "max", "last"],
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 11,
      "type": "timeseries",
      "title": "Request Rate by Status Code",
      "description": "Breakdown of HTTP responses by status code",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 25 },
      "targets": [
        {
          "expr": "sum by (http_response_status_code) (rate(http_server_requests_total[5m]))",
          "legendFormat": "HTTP {{http_response_status_code}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 30,
            "gradientMode": "none",
            "spanNulls": true,
            "stacking": {
              "mode": "normal"
            }
          },
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": "5.." },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }
            ]
          },
          {
            "matcher": { "id": "byRegexp", "options": "4.." },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" } }
            ]
          },
          {
            "matcher": { "id": "byRegexp", "options": "2.." },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "calcs": ["mean", "last"],
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 12,
      "type": "timeseries",
      "title": "Request Volume by Route",
      "description": "Traffic distribution across endpoints",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 33 },
      "targets": [
        {
          "expr": "sum by (http_route) (rate(http_server_requests_total[5m]))",
          "legendFormat": "{{http_route}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "bars",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 1,
            "fillOpacity": 80,
            "gradientMode": "none",
            "spanNulls": true
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "legend": {
          "calcs": ["mean", "max", "last"],
          "displayMode": "table",
          "placement": "right"
        }
      }
    },
    {
      "id": 400,
      "type": "row",
      "title": "🔌 Dependencies (DB & HTTP Clients)",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 41 },
      "collapsed": false
    },
    {
      "id": 13,
      "type": "timeseries",
      "title": "Database Client Latency",
      "description": "DB operation latency with exemplar links to traces",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 42 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(db_client_operation_duration_seconds_bucket[5m])))",
          "legendFormat": "p50",
          "refId": "A",
          "exemplar": true
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(db_client_operation_duration_seconds_bucket[5m])))",
          "legendFormat": "p95",
          "refId": "B",
          "exemplar": true
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(db_client_operation_duration_seconds_bucket[5m])))",
          "legendFormat": "p99",
          "refId": "C",
          "exemplar": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "spanNulls": true
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "legend": {
          "calcs": ["mean", "max", "last"],
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 14,
      "type": "timeseries",
      "title": "HTTP Client Latency",
      "description": "Outbound HTTP request latency with exemplar links",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 42 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(http_client_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p50",
          "refId": "A",
          "exemplar": true
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_client_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p95",
          "refId": "B",
          "exemplar": true
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(http_client_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p99",
          "refId": "C",
          "exemplar": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "spanNulls": true
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "legend": {
          "calcs": ["mean", "max", "last"],
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    }
  ]
}
